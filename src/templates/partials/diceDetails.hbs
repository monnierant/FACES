<div class="faces-roll-details">
  {{log roll}}
  {{#each roll.dice as |dice|}}
  {{#each dice.results as |result|}}
  <div
    class="result-dice {{ ternary (eq dice.faces result.result) 'success' '' }}"
  >
    <i class="result-dice-icon monni-icon-d{{ dice.faces }}"></i>
    <div class="result-dice-value">
      <div>
        {{ result.result }}
      </div>
    </div>
  </div>
  {{/each}}
  {{/each}}
</div>
